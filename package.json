{
  "name": "ah-jarvis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.0.1",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "concurrently \"yarn start\" \"yarn sass:watch\"",
    "sass:watch": "node-sass-chokidar ./src/sass/ -o ./src/ --watch",
    "sass:build": "node-sass-chokidar ./src/sass/ -o ./src/"
  },
  "devDependencies": {
    "enzyme": "^3.5.0"
  }
}
